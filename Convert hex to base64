import binascii

# Your hex string
hex_string = "49276d206b696c6c696e6720796f757220627261696e206c696b65206120706f69736f6e6f7573206d757368726f6f6d"

# Convert hex to bytes
byte_data = binascii.unhexlify(hex_string)

# Convert bytes to base64
